    async eliminarCita(id) {
        const cita = this.citas.find(c => c.id === id);
        if (!cita) {
            alert('Cita no encontrada');
            return;
        }

        if (!confirm(`üóëÔ∏è ¬øEst√° seguro de eliminar la cita de ${cita.paciente_completo}?\n\nFecha: ${cita.fecha_formateada}\nHora: ${cita.hora}\n\nEsta acci√≥n no se puede deshacer.`)) {
            return;
        }

        try {
            const response = await fetch(`/api/citas/${id}`, {
                method: 'DELETE',
                headers: {
                    'Content-Type': 'application/json',
                }
            });

            if (!response.ok) {
                const error = await response.json();
                throw new Error(error.msg || 'Error al eliminar cita');
            }

            alert('‚úÖ Cita eliminada correctamente');
            
            // Recargar las citas
            this.loadCitas();
            
        } catch (error) {
            console.error('Error al eliminar cita:', error);
            alert(`‚ùå Error al eliminar cita: ${error.message}`);
        }
    }

